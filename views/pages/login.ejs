            <input id="password" class="input" type="password" placeholder="enter password">
          </div>
        </div>
      </div>
    </div>

    <div class="columns">
      <div class="column">
        <div class="field">
          <div id="sumbit" class="control">
            <a class="button" type="submit" method="PUT">Log in</a>
          </div>
        </div>
      </div>
    </div>
    </div>

    <footer>
      <% include ../partials/footer %>
